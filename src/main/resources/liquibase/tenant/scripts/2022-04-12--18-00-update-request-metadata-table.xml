<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

  <changeSet id="2022-04-12--18-00-add-statistics-fields-to-request-metadata" author="Viachaslau Khandramai">
    <addColumn tableName="request_metadata_lb">
      <column name="returned_instances_counter" type="int"/>
      <column name="skipped_instances_counter" type="int"/>
      <column name="failed_instances_counter" type="int"/>
      <column name="supressed_instances_counter" type="int"/>
    </addColumn>
  </changeSet>

</databaseChangeLog>
